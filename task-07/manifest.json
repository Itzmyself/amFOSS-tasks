{
    "manifest_version": 3,
    "name": "Weather Extension",
    "version": "1.0",
    "description": "Get real-time weather data for any location.",
    "permissions": ["geolocation", "activeTab", "scripting"],
    "host_permissions": ["https://api.openweathermap.org/*"],
    "action": {
      "default_popup": "extension.html",
      "default_icon": "mrng.webp" 
    }
  }
  